<view class="pageInfo" wx:for="{{infoList}}" wx:for-item="info" wx:for-index="infoIndex">
    <view class="bookTime" wx:if="{{pageType == 'edit' && info.guideRealReimbTime}}">
        记账于 {{info.guideRealReimbTime}}
    </view>
    <view class="titleInfo" wx:if="{{pageType == 'edit'}}">
        <text class="name">{{info.name}} <text wx:if="{{resourceName == 'hotel'}}">(住{{info.times}}晚)</text></text>
        <text class="logs" bindtap="getGuideLog" data-type="{{resourceName}}" data-id="{{info.id}}">日志</text>
    </view>
    <view wx:if="{{resourceName == 'hotel'}}" class="listInfo">
        <view wx:if="{{pageType == 'edit'}}" class="editInfo">
            <view class="item">
                <label class="">房间类型：</label>
                <text class=''>{{info.hotelType}}</text>
            </view>
            <view class="item">
                <label for="">入住时间：</label>
                <text class=''>{{info.orderDate}}</text>
            </view>
            <view class="item">
                <label for="">安排备注：</label>
                <text wx:if="{{info.remark}}" class=''>{{info.remark}}</text>
            </view>
        </view>
        <view class="list">
            <view wx:if="{{pageType == 'edit'}}" class="item itemH">
                <label for="" class="itemL">客人信息</label>
                <view class="itemR" bindtap="getGroup" data-index="{{infoIndex}}">
                    <!-- <text>{{info.memberContactInfo}}</text>
                    <text class="iconfont iconarrow-right"></text> -->
                    <input class='search-input' placeholder="请选择" value="{{info.memberContactInfo}}" disabled></input>
                </view>

                <!-- <label wx:if="{{!info.customerOrderNumber && !info.customerTripNumber}}" for="" class="itemL">客人信息</label>
                <label wx:elif="{{info.customerOrderNumber && info.customerOrderNumber !=''}}" for="" class="itemL">客人信息</label>
                <label wx:elif="{{info.customerTripNumber && info.customerTripNumber !=''}}" for="" class="itemL">客人信息</label>
                <view class="itemR" bindtap="getGroup" data-index="{{infoIndex}}">
                    <text wx:if="{{!info.customerOrderNumber && !info.customerTripNumber}}"></text>
                    <text wx:elif="{{info.customerOrderNumber && info.customerOrderNumber !=''}}">{{info.customerOrderNumber}}</text>
                    <text wx:elif="{{info.customerTripNumber && info.customerTripNumber !=''}}">{{info.customerTripNumber}}</text>
                    <text class="iconfont iconarrow-right"></text>
                </view> -->
            </view>
            <view wx:if="{{pageType == 'add'}}" class="item itemH">
                <label for="" class="itemL">酒店</label>
                <view class="itemR" bindtap="getSearchList" data-method="hotel" data-type="resources" data-index="{{infoIndex}}">
                    <input class='search-input remark' bindtap="getSearchList" data-method="hotel" data-type="resources" data-index="{{infoIndex}}" placeholder="请选择" value="{{info.resourceText}}" disabled></input>
                    <!-- <text class="iconfont iconarrow-right"></text> -->
                </view>
            </view>
            <view wx:if="{{pageType == 'edit'}}" class="item itemH">
                <label for="" class="itemL">行程日期</label>
                <view class="itemR">
                    <picker class="search-input" mode="date" value="{{info.orderDate}}" start="2010-01-01" end="2030-12-31" bindchange="myinput" data-name="orderDate" data-index="{{infoIndex}}">
                        <view class="picker">{{info.orderDate}}</view>
                    </picker>
                    <!-- <text class="iconfont iconarrow-right"></text> -->
                </view>
            </view>
            <view class="item itemH">
                <label for="" class="itemL">房间类型</label>
                <view class="itemR">
                    <!-- <text class="grey" bindtap="getSearchList" data-type="dictionary" data-code="jiudianfangjian_leixing" data-index="{{infoIndex}}">请选择</text>
                    <input class='search-input' bindinput="myinput" value="{{info.dictionary}}" data-name="dictionary" data-index="{{infoIndex}}" placeholder="请输入"></input> -->
                    <!-- <text class="iconfont iconarrow-right" bindtap="getSearchList" data-type="dictionary" data-code="jiudianfangjian_leixing" data-index="{{infoIndex}}"></text> -->
                    <input class="search-input" bindtap="getSearchList" data-type="dictionary" data-code="jiudianfangjian_leixing" data-index="{{infoIndex}}" value="{{info.dictionary}}" placeholder="请选择" disabled></input>
                </view>
            </view>
            <view class="item itemH">
                <label for="" class="itemL">付款方式</label>
                <view class="itemR">
                    <picker class="search-input" mode='selector' bindchange="bindCashTypeChange" value="{{cashTypeidx}}" range="{{cashTypeList}}" range-key="{{'name'}}" data-selecthx="{{cashTypeList[cashTypeidx].name}}" data-index="{{infoIndex}}">
                        <view class="picker">{{info.cashTypeName}}</view>
                    </picker>
                    <!-- <text class="iconfont iconarrow-right"></text> -->
                </view>
            </view>
            <view wx:if="{{cashTypeidx == 0}}" class="item itemH" bindtap="getSignNumberList">
                <label for="" class="itemL">签单号</label>
                <view class="itemR" bindtap="getSignNumberList" data-index="{{infoIndex}}">
                    <!-- <text class='search-input'>{{info.signNumber}}</text> -->
                    <!-- <text class="iconfont iconarrow-right"></text> -->
                    <input class='search-input' placeholder="请选择" value="{{info.signNumber}}" disabled></input>
                </view>
            </view>
            <view class="item itemH">
                <label for="" class="itemL">天数</label>
                <view class="itemR">
                    <input class='search-input' bindinput="myinput" value="{{info.times}}" data-name="times" data-index="{{infoIndex}}" placeholder="请输入"></input>
                </view>
            </view>
            <view class="item itemH">
                <label for="" class="itemL">单价</label>
                <view class="itemR">
                    <input class='search-input' bindinput="myinput" value="{{info.price}}" data-name="price" data-index="{{infoIndex}}" placeholder="请输入"></input>
                </view>
            </view>
            <view wx:if="{{pageType == 'add'}}" class="item itemH">
                <label for="" class="itemL">客人信息</label>
                <view class="itemR">
                    <text wx:if="{{memberList.length != 0}}" class='search-input' bindtap="getGroup" data-index="{{infoIndex}}">共{{memberList.length}}组</text>
                    <text wx:if="{{memberList.length == 0}}" class='search-input' bindtap="getGroup" data-index="{{infoIndex}}">请选择</text>
                    <!-- <text class="iconfont iconarrow-right"></text> -->
                </view>
            </view>
            <view wx:if="{{memberList.length != 0}}" class="allChild">
                <view class="childLsit">
                    <view class="childItem" wx:for="{{memberList}}" wx:key='{{index}}'>
                        <view class="itemInfo">
                            <label for="" class="">客人</label>
                            <view class="itemR">
                                <text class='search-input remark'>{{item.memberContactInfo}}</text>
                                <text class="iconfont iconguanbi" bindtap='delChildMemberItem' data-idx='{{index}}' data-index="{{infoIndex}}"></text>
                            </view>

                            <!-- <label wx:if="{{!item.orderNumber && !item.customerTripNumber}}" for="" class="">小团号</label>
                            <label wx:elif="{{item.orderNumber && item.orderNumber !=''}}" for="" class="">小团号</label>
                            <label wx:elif="{{item.customerTripNumber && item.customerTripNumber !=''}}" for="" class="">流水号</label>
                            <view class="itemR">
                                <text wx:if="{{!item.orderNumber && !item.customerTripNumber}}" class='search-input remark'></text>
                                <text wx:elif="{{item.orderNumber && item.orderNumber !=''}}" class='search-input remark'>{{item.orderNumber}}</text>
                                <text wx:elif="{{item.customerTripNumber && item.customerTripNumber !=''}}" class='search-input remark'>{{item.customerTripNumber}}</text>
                                <text class="iconfont iconguanbi" bindtap='delChildMemberItem' data-idx='{{index}}'></text>
                            </view> -->
                        </view>
                        <view class="itemInfo">
                            <label for="" class="">数量</label>
                            <view class="">
                                <input class='search-input' bindinput="getMemberKeyWord" value="{{item.count}}" data-idx='{{index}}' data-name="count" placeholder="请输入"></input>
                            </view>
                        </view>
                        <view class="itemInfo">
                            <label for="" class="">免去数量</label>
                            <view class="">
                                <input class='search-input' bindinput="getMemberKeyWord" value="{{item.freeCount}}" data-idx='{{index}}' data-name="freeCount" placeholder="请输入"></input>
                            </view>
                        </view>
                        <view wx:if="{{cashTypeidx == 0}}" class="itemInfo">
                            <label for="" class="">签单金额</label>
                            <view class="">
                                <input class='search-input' bindinput="getMemberKeyWord" value="{{item.guideRealReimbMoney}}" data-idx='{{index}}' data-name="guideRealReimbMoney" placeholder="请输入"></input>
                            </view>
                        </view>
                        <view wx:if="{{cashTypeidx == 1}}" class="itemInfo">
                            <label for="" class="">现付金额</label>
                            <view class="">
                                <input class='search-input' bindinput="getMemberKeyWord" value="{{item.guideRealReimbMoney}}" data-idx='{{index}}' data-name="guideRealReimbMoney" placeholder="请输入"></input>
                            </view>
                        </view>
                        <view class="itemInfo">
                            <label for="" class="">记账备注</label>
                            <view class="itemR">
                                <!-- <text class='search-input remark' bindtap="getRemark" data-idx='{{index}}' data-name="remark" data-value="{{item.remark}}">{{item.remark}}</text> -->
                                <!-- <text class="iconfont iconarrow-right"></text> -->
                                <input class='search-input remark' bindtap="getRemark" data-idx='{{index}}' data-name="remark" data-value="{{item.remark}}" placeholder="请输入" value="{{item.remark}}" disabled></input>
                            </view>
                        </view>
                    </view>
                </view>
            </view>  
            <view wx:if="{{memberList.length == 0}}" class="item itemH">
                <label for="" class="itemL">数量</label>
                <view class="itemR">
                    <input class='search-input' bindinput="myinput" value="{{info.count}}" data-name="count" data-index="{{infoIndex}}" placeholder="请输入"></input>
                </view>
            </view>
            <view wx:if="{{memberList.length == 0}}" class="item itemH">
                <label for="" class="itemL">免去数量</label>
                <view class="itemR">
                    <input class='search-input' bindinput="myinput" value="{{info.freeCount}}" data-name="freeCount" data-index="{{infoIndex}}" placeholder="请输入"></input>
                </view>
            </view>
            <view wx:if="{{cashTypeidx == 0 && memberList.length == 0}}" class="item itemH">
                <label for="" class="itemL">签单金额</label>
                <view class="itemR">
                    <input class='search-input' bindinput="myinput" value="{{info.guideRealReimbMoney}}" data-name="guideRealReimbMoney" data-index="{{infoIndex}}" placeholder="请输入"></input>
                </view>
            </view>
            <view wx:if="{{cashTypeidx == 1 && memberList.length == 0}}" class="item itemH">
                <label for="" class="itemL">现付金额</label>
                <view class="itemR">
                    <input class='search-input' bindinput="myinput" value="{{info.guideRealReimbMoney}}" data-name="guideRealReimbMoney" data-index="{{infoIndex}}" placeholder="请输入"></input>
                </view>
            </view>
            <view wx:if="{{memberList.length == 0}}" class="item itemH">
                <label for="" class="itemL">记账备注</label>
                <view class="itemR">
                    <!-- <text class='search-input remark' bindtap="getRemark" data-name="guideRealReimbRemark" data-value="{{info.guideRealReimbRemark}}" data-index="{{infoIndex}}">{{info.guideRealReimbRemark}}</text> -->
                    <!-- <text class="iconfont iconarrow-right"></text> -->
                    <input class='search-input remark' bindtap="getRemark" data-name="guideRealReimbRemark" data-value="{{info.guideRealReimbRemark}}" data-index="{{infoIndex}}" placeholder="请输入" disabled value="{{info.guideRealReimbRemark}}"></input>
                </view>
            </view>

            <view class="item">
                <view class='imgContent'>
                    <view class='imgBox'>
                        <view class='imgList'>
                        <block wx:for="{{info.detailPics}}" wx:key="index">
                            <view class='imgItem'>
                            <image src='{{item}}' data-src="{{item}}" bindtap="previewImage" data-id='{{index}}' data-index="{{infoIndex}}"></image>
                            <view wx:if="{{pageType == 'add' || reimbStatus == 'confirm' || reimbStatus == 'back'}}" class="delImg iconfont iconguanbi" bindtap="delImg" data-indx="{{index}}" data-index="{{infoIndex}}"></view>
                            </view>
                        </block>
                        <view wx:if="{{pageType == 'add' || reimbStatus == 'confirm' || reimbStatus == 'back'}}" class='chooseimg' bindtap='uploadDetailImage' data-index="{{infoIndex}}">
                            <view class="weui-uploader__input-box"></view>
                        </view>
                        </view>
                    </view>
                </view>
            </view>
        </view>
    </view>
